{
  "name": "opendaw-studio-boxes",
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "gen": "npm run clear && npm run build --prefix ../../lib/box-forge && npx tsx src/forge.ts && npm run format",
    "clear": "rm -rf ../src/data/boxes/*",
    "format": "prettier --write \"../src/data/boxes/**/*.ts\" --ignore-path /dev/null"
  },
  "devDependencies": {
    "std": "file:../../lib/std",
    "dsp": "file:../../lib/dsp",
    "box": "file:../../lib/box",
    "box-forge": "file:../../lib/box-forge",
    "tsx": "^4.19.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "@types/node": "^22.15.17"
  }
}