{
  "name": "opendaw-bundle-worklet",
  "version": "0.0.1",
  "description": "Audio worklet package for OpenDAW",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": ["dist/**/*", "package.json"],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "watch": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["audio", "worklet", "dsp", "web-audio"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/audioworklet": "^0.0.77",
    "lib-std": "*",
    "lib-dsp": "*",
    "lib-box": "*",
    "lib-runtime": "*",
    "lib-fusion": "*",
    "studio-enums": "*",
    "studio-boxes": "*",
    "studio-shared": "*"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "tsup": "^8.4.0",
    "vitest": "^3.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
