{
  "name": "@opendaw/bundle-main",
  "version": "0.0.1",
  "description": "OpenDAW main thread bundle",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "watch": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "echo \"no tests yet\"",
    "prepublishOnly": "npm run build",
    "pack:test": "npm pack",
    "test:local": "npm run build && npm pack && echo 'Package ready for local testing'"
  },
  "keywords": [
    "audio",
    "opendaw",
    "music",
    "daw",
    "main-thread",
    "adapters"
  ],
  "author": "Andre Michelle",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/andre-michelle/opendaw-studio.git",
    "directory": "packages/bundle-main"
  },
  "dependencies": {
    "@types/audioworklet": "^0.0.77"
  },
  "devDependencies": {
    "lib-std": "workspace:*",
    "lib-dsp": "workspace:*",
    "lib-box": "workspace:*",
    "lib-runtime": "workspace:*",
    "lib-fusion": "workspace:*",
    "studio-enums": "workspace:*",
    "studio-boxes": "workspace:*",
    "studio-adapters": "workspace:*"
  },
  "peerDependencies": {
    "lib-std": "*",
    "lib-dsp": "*",
    "lib-box": "*",
    "lib-runtime": "*",
    "lib-fusion": "*",
    "studio-enums": "*",
    "studio-boxes": "*",
    "studio-adapters": "*"
  },
  "overrides": {
    "lib-std": "npm:@opendaw/bundle-main",
    "lib-dsp": "npm:@opendaw/bundle-main",
    "lib-box": "npm:@opendaw/bundle-main",
    "lib-runtime": "npm:@opendaw/bundle-main",
    "lib-fusion": "npm:@opendaw/bundle-main",
    "studio-enums": "npm:@opendaw/bundle-main",
    "studio-boxes": "npm:@opendaw/bundle-main",
    "studio-adapters": "npm:@opendaw/bundle-main"
  },
  "publishConfig": {
    "access": "public"
  }
} 