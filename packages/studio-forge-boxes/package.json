{
  "name": "opendaw-forge-boxes",
  "version": "0.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run clear && npx tsx src/forge.ts && npm run format",
    "clear": "rm -rf ../studio-boxes/src/*",
    "format": "prettier --write \"../studio-boxes/src/**/*.ts\" --ignore-path /dev/null"
  },
  "devDependencies": {
    "lib-std": "*",
    "lib-box": "*",
    "lib-box-forge": "*",
    "lib-dsp": "*",
    "studio-enums": "*",
    "studio-shared": "*"
  }
}