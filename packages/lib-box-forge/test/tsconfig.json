  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ]
  }
}
