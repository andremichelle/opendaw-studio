{
  "name": "lib-box-forge",
  "version": "0.0.0",
  "description": "Source generator for box classes",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "exports": {
    ".": "./dist/index.cjs"
  },
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf test/gen/*",
    "gen": "tsx test/schema.ts",
    "test": "npm run clean && npm run gen && vitest --run test/forge.test.ts"
  },
  "author": "",
  "devDependencies": {
    "lib-box": "*",
    "lib-dom": "*",
    "lib-std": "*",
    "prettier": "^3.5.3",
    "ts-morph": "^25.0.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "vitest": "^3.1.3"
  },
  "files": [
    "dist/**/*",
    "package.json"
  ]
}